{
  "useTabs": false,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "bracketSpacing": true,
  "printWidth": 80,
  "overrides": [
    {
      "files": ["**/*.css", "**/*.scss", "**/*.html", "**/*.json"],
      "options": {
        "singleQuote": false
      }
    }
  ],
  "endOfLine": "auto",
  "plugins": ["prettier-plugin-organize-imports", "prettier-plugin-tailwindcss"]
}
